{
  "manifest_version": 3,
  "name": "Quorum",
  "version": "1.0.0",
  "description": "Get consensus-based answers from multiple AI agents.",
  "permissions": [
    "storage",
    "sidePanel",
    "identity"
  ],
  "oauth2": {
    "client_id": "697955456206-5egv95aajp4766aoq1cs14i65s69l6qv.apps.googleusercontent.com",
    "scopes": ["openid", "email", "profile"]
  },
  "host_permissions": [
    "http://localhost:5000/*",
    "https://*/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "side_panel": {
    "default_path": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
